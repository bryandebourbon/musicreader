modules = ["nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["github:1.0.0", "javascript_object_storage:2.0.0"]

[nix]
channel = "stable-25_05"
packages = ["mmh"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "eMusicReader"

[[workflows.workflow]]
name = "eMusicReader"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx serve -l 5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "autoscale"
run = ["node", "server.js"]
